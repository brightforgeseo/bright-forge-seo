---
import MainLayout from '../layouts/MainLayout.astro';
---

<MainLayout title="Test Image">
  <div class="container mx-auto p-4">
    <h1 class="text-2xl font-bold mb-4">Test Case Study Images</h1>
    
    <div class="grid gap-4">
      <div>
        <h2>Consulting Firm Image</h2>
        <img 
          src="/images/case-studies/SEO Case Study- Increasing Organic Traffic for a Consulting Firm .webp" 
          alt="Consulting Firm" 
          width="400" 
          height="300"
        />
      </div>
      
      <div>
        <h2>Speediance AU Image</h2>
        <img 
          src="/images/case-studies/SEO Case Study- Increasing Organic Traffic for Speediance AU â€“ Fitness Equipment Supplier .webp" 
          alt="Speediance AU" 
          width="400" 
          height="300"
        />
      </div>
      
      <h1 class="text-2xl font-bold mt-8 mb-4">Proudly Featured On Images</h1>
      <div class="grid gap-8">
        <div>
          <h2>Design Rush Logo</h2>
          <img 
            src="/images/proud/design-rush-logo.png" 
            alt="Design Rush" 
            class="h-28 w-auto object-contain" 
            width="200" 
            height="80"
          />
        </div>
        
        <div>
          <h2>Upwork Logo</h2>
          <img 
            src="/images/proud/2021-upwork-new-logo-design.png" 
            alt="Upwork" 
            class="h-28 w-auto object-contain" 
            width="200" 
            height="80"
          />
        </div>
        
        <div>
          <h2>Clutch Logo</h2>
          <img 
            src="/images/proud/66ede09890c1d16a8ccb2d2c_1.png" 
            alt="Clutch" 
            class="h-28 w-auto object-contain" 
            width="200" 
            height="80"
          />
        </div>
      </div>
    </div>
  </div>
</MainLayout>
