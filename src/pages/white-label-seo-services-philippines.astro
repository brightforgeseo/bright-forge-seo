<style>
/* White Label Icon Grid (used in Hero visual slot) */
.hero-image-container {
  position: relative;
  width: 100%;
  max-width: 500px;
}

.white-label-icon-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 2rem;
  padding: 2rem;
}

.white-label-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 80px;
  height: 80px;
  background: rgba(255, 255, 255, 0.15);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 20px;
  color: white;
  font-size: 2rem;
  transition: all 0.3s ease;
  animation: float 4s ease-in-out infinite;
}

@keyframes float {
  0%, 100% { transform: translateY(0px); }
  50% { transform: translateY(-10px); }
}

.white-label-icon:nth-child(odd) {
  animation-delay: 1s;
}

.white-label-icon:nth-child(even) {
  animation-delay: 2s;
}

.white-label-icon:hover {
  transform: translateY(-5px) scale(1.05);
  background: rgba(255, 255, 255, 0.25);
  border-color: rgba(255, 255, 255, 0.4);
}

@media (max-width: 1024px) {
  .white-label-icon-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 1.5rem;
  }
}

@media (max-width: 768px) {
  .white-label-icon-grid {
    grid-template-columns: repeat(3, 1fr);
    gap: 1rem;
  }

  .white-label-icon {
    width: 60px;
    height: 60px;
    font-size: 1.5rem;
  }
}

/* Why Section */
.why-section {
  padding: 5rem 0;
  background: white;
}

.section-header {
  text-align: center;
  margin-bottom: 3rem;
}

.section-header h2 {
  font-size: 2.5rem;
  font-weight: 800;
  color: #1a1a1a;
  margin-bottom: 1rem;
  line-height: 1.2;
}

.section-header h2 span {
  background: linear-gradient(135deg, #f97316, #FF6B35);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.content-explanation {
  display: flex;
  justify-content: center;
}

.content-card {
  display: flex;
  max-width: 1000px;
  background: #f8f9fa;
  border-radius: 16px;
  overflow: hidden;
  box-shadow: 0 5px 15px rgba(0,0,0,0.08);
}

.content-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  min-width: 120px;
  background: linear-gradient(135deg, #f97316, #FF6B35);
  color: white;
  font-size: 2.5rem;
}

.content-text {
  padding: 2.5rem;
}

.content-text p {
  font-size: 1.05rem;
