# Copy this file to .env (do not commit .env) and fill in your values
# PayPal public config for client-side subscriptions
PUBLIC_PAYPAL_CLIENT_ID=

# Live plan IDs for monthly subscriptions (from PayPal dashboard, begin with "P-")
PUBLIC_PAYPAL_PLAN_ID_STARTER=
PUBLIC_PAYPAL_PLAN_ID_GROWTH=
PUBLIC_PAYPAL_PLAN_ID_FAST=

# Optional overrides
PUBLIC_PAYPAL_CURRENCY=PHP
PUBLIC_PAYPAL_SUCCESS_URL=/thanks
PUBLIC_PAYPAL_CANCEL_URL=/?payment=cancel
